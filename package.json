{
  "name": "minerador",
  "version": "1.0.0",
  "description": "Demonstração Maré Analitica",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "setup:db": "npx prisma db push",
    "start": "npx tsx ./src/app.ts",
    "build": "tsc",
    "start:prod": "node ./dist/app.js",
    "start:pm2": "npx pm2 start ./dist/app.js",
    "stop:pm2": "npx pm2 kill",
    "pack": "npx nexe -i ./dist/app.js -r ./node_modules -t x64-14.15.3 -o ./scripts/demonstracao.exe"
  },
  "keywords": [
    "rpa",
    "minerador",
    "dados abertos"
  ],
  "author": "Paulo Victor <mareanalitica@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.2.0",
    "@types/axios": "^0.14.0",
    "axios": "^1.5.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "queue": "^7.0.0",
    "sqlite3": "^5.1.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
